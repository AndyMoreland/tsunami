{
  "name": "tsunami-code",
  "displayName": "tsunami-code",
  "description": "Tsunami integration for vscode",
  "version": "0.0.1",
  "publisher": "Derander",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onCommand:extension.importSymbol",
    "onCommand:extension.reindexProject"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "tsunami.importSymbol",
        "title": "(tsu) Import Symbol"
      },
      {
        "command": "tsunami.reindexProject",
        "title": "(tsu) Reindex Exports"
      }
    ],
    "keybindings": [
      {
        "command": "tsunami.importSymbol",
        "key": "alt+enter",
        "mac": "alt+enter",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@derander/tsunami": "*"
  }
}
